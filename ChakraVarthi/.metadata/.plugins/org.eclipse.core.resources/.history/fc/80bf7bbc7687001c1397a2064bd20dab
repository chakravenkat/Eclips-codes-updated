package org.array;

public class Aarray {
	

	public static void main(String[] args) {

		int a[] = new int[8];
		a[0] = 25;
		a[1] = 35;
		a[2] = 45;
		a[3] = 55;
		a[4] = 65;
		a[5] = 75;
		a[6] = 105;
		a[7] = 758;

		System.out.println(a[5]);
		System.out.println(a.length);
		for (int i = 0; i < 5; i++) {
		
		System.out.println(a [i] );
	}
}
}
