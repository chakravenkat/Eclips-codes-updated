package com.caseSwitch;

public class Switch{
	public static void main(String[] args) {
		int a= 4;
		switch  (a){
		case 1 :
			System.out.println("A R Rahman");
			break ;
		
		case 2 :
			System.out.println("U1");
			break;
			
		case 3:
			System.out.println("Harris Jeyaraj");
			break;
			
		case 4:
			System.out.println("G V Prakash");
			break;
		case 5:
			System.out.println("Jipran");
			
	}

	}
	}
