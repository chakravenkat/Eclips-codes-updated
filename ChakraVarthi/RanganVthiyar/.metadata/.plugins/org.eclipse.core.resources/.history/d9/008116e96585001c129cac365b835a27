package org.loops;



public class DoWhile {
	public static void main(String[] args) {
		System.out.println("India Vs Pakistan First ODI");
		System.out.println("  ");
	
	     int i=248;
			
			
			if(i>240)
			{
		
			System.out.print("India Won The Match");
			}
		
			else if (i>248 && i==242)
	
				System.out.print("Match Draw");
			
				else  {
		
				System.out.print("India loss the Match");
				
			}
				
		
	}

}
