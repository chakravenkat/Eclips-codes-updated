package com.concept;

public class matric {
	public static void main(String[] args) {
		int a[][]= {{1,3},{4,5}};
		int b[][]= {{6,7},{8,9}};
		int c[][]= new int[2][2];
		for(int i=0;i<=2;i++) {
			for(int j=0;j<2;j++) {
				System.out.print(a[i][j]+b[i[j]+" ");
			}
			System.out.println();
		}
	}

}
